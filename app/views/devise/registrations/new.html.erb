<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="well">

      <h2>Registration Form</h2>
      
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
        
        <div class="field form-group">
          <%= f.label :Username %><br />
          <%= f.text_field :Username, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :First_Name %><br />
          <%= f.text_field :First_Name, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :Last_Name %><br />
          <%= f.text_field :Last_Name, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :email %><br />
          <%= f.email_field :email, autofocus: true, class: 'form-control' %>
        </div>
      
        <div class="field form-group">
          <%= f.label :password %>
          <% if @validatable %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off", class: 'form-control' %>
        </div>
      
        <div class="field form group">
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :Age %><br />
          <%= f.text_field :Age, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :Gender %><br />
          <%= f.text_field :Gender, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :Ethnicity %><br />
          <%= f.text_field :Ethnicity, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :State %><br />
          <%= f.text_field :State, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :City_or_Town %><br />
          <%= f.text_field :City_or_Town, autofocus: true, class: 'form-control' %>
        </div>
        <div class="field form-group">
          <%= f.label :Born_and_Raised %><br />
          <%= f.text_field :Born_and_Raised, autofocus: true, class: 'form-control' %>
        </div>
      
        
      
        <div class="actions">
          <%= f.submit "Register", class: 'btn btn-success' %>
        </div>
      <% end %>
      
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>